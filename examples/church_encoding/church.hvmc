//@main
  //= R
  //& (a a) ~ ((b b) R)




@S = ((a (b c)) ({2 a (c d)} (b d)))
@Z = (* (a a))

@add = ((a (b c)) ((d (e b)) ({2 d a} (e c))))
@mul = ((a (b c)) ((d a) (d (b c))))

@c2 = ({2 (a b) (b c)} (a c))
@c3 = ({2 (a b) {3 (b c) (c d)}} (a d))

@c4
  = a
  & @S ~ (b a)
  & @S ~ (c b)
  & @S ~ (d c)
  & @S ~ (@Z d)

@main
  = a
  & @add ~ (b (@c4 a))
  & @mul ~ (@c2 (@c3 b))

